# Evitar errores por simbolos
escape_latex <- function(x) {
x <- gsub("_", "\\\\_", x)
x <- gsub("&", "\\\\&", x)
x <- gsub("%", "\\\\%", x)   # <- aquí se protege el %
x <- gsub("#", "\\\\#", x)
x <- gsub("\\$", "\\\\$", x)
x <- gsub("\\{", "\\\\{", x)
x <- gsub("\\}", "\\\\}", x)
x <- gsub("\\\\", "\\\\textbackslash{}", x)
x <- gsub("~", "\\\\textasciitilde{}", x)
x <- gsub("\\^", "\\\\textasciicircum{}", x)
x <- gsub("\\|", "\\\\textbar{}", x)
return(x)
}
